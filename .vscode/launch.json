    "version": "0.2.0",
    "configurations": [
        {
            "request": "launch",
            "name": "C/C++: gdb Debugger",
            "type": "cppdbg",
            "program": "${workspaceFolder}/etapa4", // Replace with your executable path
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb", // Adjust path to your GDB
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}